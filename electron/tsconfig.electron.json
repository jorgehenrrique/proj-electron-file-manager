  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "outDir": "../dist-electron",
    "rootDir": ".",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "allowJs": true,
    "resolveJsonModule": true
  },
  "include": [
    "**/*",
  ]
}